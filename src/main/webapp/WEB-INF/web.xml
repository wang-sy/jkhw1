<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>cqu.edu.cn.ssosystem.servlet.UserServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>Token</servlet-name>
        <servlet-class>cqu.edu.cn.ssosystem.servlet.TokenServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>Rsa</servlet-name>
        <servlet-class>cqu.edu.cn.ssosystem.servlet.RsaServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Token</servlet-name>
        <url-pattern>/token</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Rsa</servlet-name>
        <url-pattern>/rsa</url-pattern>
    </servlet-mapping>

</web-app>
